{
  "name": "canteenmenuparser-html",
  "version": "1.0.0",
  "description": "the html for the canteen menu parser",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildScss": "node-sass -r -o app app",
    "buildTs": "tsc -p .",
    "buildTs:w": "npm run buildTs -- -w",
    "buildScss:w": "node-sass -r -w -o ./app ./app",
    "build:w": "npm run buildScss:w & npm run buildTs:w",
    "bs": "browser-sync start --config bs-config.js",
    "serve": "npm run bs & npm run build:w"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.3.1",
    "@types/jquery-mousewheel": "^3.1.8",
    "node-sass": "^4.8.3"
  }
}
